<div class="container">
  <h1>My Lists</h1>
  <div class="cards">
    <% @lists.each do |list| %>
      <%= link_to list_path(list) do %>
<div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url('<%= cl_image_path list.photo.key, height: 300, crop: :fill %>')">          <%= list.name %>
        </div>
      <% end %>
    <% end %>
  </div>
  <%= link_to "Add a new list", new_list_path, class: "btn btn-primary" %>
</div>
